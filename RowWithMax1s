	int rowWithMax1s(vector<vector<int> > arr, int n, int m) {
	    int max_cnt=0;
	    int max_row=-1;
	    for(int i=0;i<n;i++){
	        int cnt=0;
	        for(int j=0;j<m;j++){
	            if(arr[i][j]==1)
	               cnt++;
	               
	        }
	        if(cnt>max_cnt){
	        max_cnt=cnt;
	        max_row=i;
	        }
	    }
	    return max_row;
	   
	    
	}
